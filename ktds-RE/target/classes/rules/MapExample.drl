package rule
import java.util.Map

rule "고객 룰1"
    no-loop true
    lock-on-active true
    salience 1
    when
        $map : Map(
		this["SEX_TYPE_NM"] == "남" && 
		this["CUST_AGE"] > 30 && 
		this["CUST_CLAS_NM"] == "GOLD"  )
    then
        $map.put("RETURN", "Y");
end

rule "고객룰2"
    no-loop true
    lock-on-active true
    salience 1
    when
        $map : Map(
		this["SMPH_USE_YN"] == "Y"  )
    then
        $map.put("RETURN", "Y");
end